<h1>Articles</h1>
<%= link_to "New Article", new_article_path %>
<ul>
  <% @articles.each do |article| %>
    <li>
      <%= article.title %>
      <%= link_to "show", article %>
      <%= link_to "Edit", edit_article_path(article) %>
      <%= button_to "Destroy",article_path(article), :method => :delete %>
    </li>
  <% end %>
</ul>

